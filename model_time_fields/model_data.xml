<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>
    <!-- res.partner -->
    <record id="action_time_fields" model="ir.actions.server">
      <field name="name">Measure read times</field>
      <!--
           <field name="model_id" ref="product.model_product_template"/>
      -->
      <field name="state">code</field>
      <field name="code">
        res = []
        for field in object.fields_get():
            start = datetime.datetime.now()
            object.read([field])
            dt = datetime.datetime.now() - start
            res.append((float(dt.seconds) + float(dt.microseconds) / 1000000, field))
            res.sort(key=lambda f: f[0], reverse=True)
        raise Warning('\n'.join(['%.6f s:\t%s' % (t[0], '\t' + t[1] + '.'*(35-len(t[1]))) for t in res]))
      </field>
    </record>
    <record id="profile_form" model="ir.ui.view">
      <field name="name">profile_model.form</field>
      <field name="model">profile.model</field>
      <field name="arch" type="xml">
        <form string="Profile form">
          <group>
            <field name="model" />
            <field name="profile_field" />
          </group>
          <footer>
            <button name="go_step_1" string="Next" type="object" class="oe_highlight" />
            or
            <button string="Cancel" class="oe_link" special="cancel" />
          </footer>
        </form>
      </field>
    </record>
    <record id="action_profile_form" model="ir.actions.act_window">
      <field name="name">Profile model</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">profile.model</field>
      <field name="src_model">product.template</field>
      <field name="view_type">form</field>
      <field name="view_mode">form</field>
      <field name="view_id" ref="profile_form" />
      <field name="target">new</field>
      <field name="context">False</field>
    </record>
    
  </data>
</openerp>